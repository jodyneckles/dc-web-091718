<%= render partial: "shared/error_list", locals: {object: @snack} %>

<%= form_for @snack do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :calories %>
    <%= f.number_field :calories %>
    <%= f.label :deliciousness %>
    <%= f.select :deliciousness, (1..10) %>
    <%= f.collection_select(:retailer_id, Retailer.all, :id, :name, prompt: "Select a retailer") %>

    <%= f.submit "Snackify" %>
<% end %>